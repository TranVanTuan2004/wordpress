    transform: translateY(-132px);
    transition: transform 0.5s ease;
    line-height: 1.4;
    white-space: normal;
    box-sizing: border-box;
    font-size: 16px;
}

.movie-notice-link {
    color: #fff;
    text-decoration: none;
    transition: color 0.3s ease;
}

.movie-notice-link:hover {
    color: rgba(80, 79, 79, 0.7);
}

.form-search {
    background-color: transparent !important;
}

/* ===== CLEAN CINEMA DROPDOWN STYLES ===== */
.cinema-dropdown-item {
    position: relative;
}

.navbar .menu .cinema-dropdown {
    display: none;
    position: absolute;
    top: calc(100% + 12px);
    left: 0;
    width: 900px;
    border: 1px solid rgba(255, 228, 77, 0.3);
    z-index: 999;
    padding: 0;
    border-radius: 16px;
    background: linear-gradient(135deg, rgba(14, 18, 32, 0.98) 0%, rgba(7, 30, 61, 0.98) 100%);
    backdrop-filter: blur(20px);
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.5), 0 0 0 1px rgba(255, 228, 77, 0.1);
    overflow: hidden;
    opacity: 0;
    transform: translateY(-10px);
    transition: opacity 0.3s ease, transform 0.3s ease;
}

.cinema-dropdown-item:hover>.cinema-dropdown {
    display: flex;
    flex-direction: column;
    opacity: 1;
    transform: translateY(0);
}

.cinema-dropdown .dropdown-header {
    background: linear-gradient(135deg, #ffe44d 0%, #ffd700 100%);
    padding: 16px 24px;
    display: flex;
    align-items: center;
    gap: 12px;
    border-bottom: 2px solid rgba(255, 228, 77, 0.3);
}

.cinema-dropdown .dropdown-header i {
    font-size: 20px;
    color: #0e1220;
}

.cinema-dropdown .dropdown-header span {
    font-size: 16px;
    font-weight: 700;
    color: #0e1220;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

.cinema-dropdown>div:not(.dropdown-header) {
    display: flex;
    padding: 20px;
    gap: 0;
}

.cinema-dropdown .dropdown-column {
    flex: 1;
    min-width: 280px;
    padding: 0 12px;
}

.cinema-dropdown .dropdown-column:not(:last-child) {
    border-right: 1px solid rgba(255, 255, 255, 0.1);
}

.cinema-dropdown .dropdown-column a.cinema-link {
    display: flex;
    align-items: center;
    gap: 18px;
    padding: 12px 16px;
    font-size: 15px;
    color: #e5e7eb;
    border-radius: 10px;
    transition: all 0.2s ease;
    margin-bottom: 4px;
    position: relative;
    overflow: hidden;
}

.cinema-dropdown .dropdown-column a.cinema-link::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 3px;
    background: linear-gradient(135deg, #ffe44d 0%, #ffd700 100%);
    transform: scaleY(0);
    transition: transform 0.2s ease;
}

.cinema-dropdown .dropdown-column a.cinema-link:hover::before {
    transform: scaleY(1);
}

.cinema-dropdown .dropdown-column a.cinema-link i {
    font-size: 18px;
    color: rgba(255, 228, 77, 0.7);
    transition: all 0.2s ease;
    width: 20px;
    text-align: center;
    flex-shrink: 0;
}

.cinema-dropdown .dropdown-column a.cinema-link:hover {
    background: rgba(255, 228, 77, 0.1);
    color: #ffe44d;
    transform: translateX(4px);
}

.cinema-dropdown .dropdown-column a.cinema-link:hover i {
    color: #ffe44d;
    transform: scale(1.1);
}

.cinema-dropdown .dropdown-column a.cinema-link span {
    font-weight: 500;
    flex: 1;
}