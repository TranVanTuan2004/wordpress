    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.5), 0 0 0 1px rgba(255, 228, 77, 0.1);
    overflow: hidden;
    opacity: 0;
    transform: translateY(-10px);
    transition: opacity 0.3s ease, transform 0.3s ease;
}

.cinema-dropdown-item:hover>.cinema-dropdown {
    display: flex;
    flex-direction: column;
    opacity: 1;
    transform: translateY(0);
}

.cinema-dropdown .dropdown-header {
    background: linear-gradient(135deg, #ffe44d 0%, #ffd700 100%);
