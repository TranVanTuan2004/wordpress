}

.cinema-dropdown>div:not(.dropdown-header) {
    .cinema-dropdown .dropdown-column a.cinema-link i {
        font-size: 18px;
        color: rgba(255, 228, 77, 0.7);
        transition: all 0.2s ease;
        width: 20px;
        text-align: center;
    }

    .cinema-dropdown .dropdown-column a.cinema-link:hover {
        background: rgba(255, 228, 77, 0.1);
        color: #ffe44d;
        transform: translateX(4px);
    }

}

